<?xml version="1.0" encoding="utf-8"?>
<CAT>
    <module name="test">
        <!--测试模块-->
        <state>on</state>
        <templete platform="XM_IPC">/etc/cat/xm_ipc.xml</templete>
        <templete platform="HaiKang_xx">/etc/cat/hk_xx.xml</templete>
    </module>
	
    <module name="report">
        <!--报告模块-->
        <state>on</state>
        <output>STDOUT</output>
    </module>
	
    <module name="erp">
        <!--ERP模块-->
        <state>off</state>
	</module>

    <module name="gui">
        <!--GUI模块-->
        <state>off</state>
    </module>
	
    <module name="log">
        <!--日志-->
        <state>on</state>
        <!--0: off, 1: fatal, 2: error, 3: warn, 4:info, 5:debug-->
        <level>4</level>
        <file>/tmp/cat.log</file>
    </module>
</CAT>

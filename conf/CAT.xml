<?xml version="1.0" encoding="utf-8"?>
<CAT>
	<module name="test">
		<!--测试模块-->
		<state>on</state>
		<test_templete>/etc/cat/test_templete.xml</test_templete>
	</module>
	
	<module name="report">
		<!--报告模块-->
		<state>on</state>
		<output>STDOUT</output>
	</module>
	
	<module name="erp">
		<!--ERP模块-->
		<state>off</state>
	</module>

	<module name="gui">
		<!--GUI模块-->
		<state>off</state>
	</module>
	
	<module name="log">
		<!--日志-->
		<state>on</state>
        <!--0: fatal, 1: error, 2: warn, 3:info, 4:debug-->
        <level>3</level>
		<file>/tmp/cat.log</file>
	</module>
</CAT>